{
  "name": "bhoomisetu",
  "version": "1.0.0",
  "description": "BhoomiSetu: A Decentralized Land Record & Property Rights Management System",
  "scripts": {
    "server:install": "cd server && npm install",
    "chaincode:install": "cd chaincode/land-registry && npm install",
    "install:all": "npm run server:install && npm run chaincode:install",
    "server:dev": "cd server && npm run dev",
    "rag:dev": "python bhoomika/simple_rag_server.py",
    "client:dev": "cd client && npm run dev",
    "start:backend": "concurrently \"npm run server:dev\" \"npm run rag:dev\"",
    "start:frontend": "npm run client:dev",
    "chaincode:build": "cd chaincode/land-registry && npm run build"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "ethereum",
    "land-registry",
    "india"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}